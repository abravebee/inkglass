{
  "name": "inkglass",
  "version": 2,
  "builds": [
   { 
     "src": "back-end/journalapi/index.js",
     "use": "@now/node"  
    },
    { "src": "**/*.js", "use": "@now/node" },
    {
      "src": "front-end/src/index.js",
      "use": "@now/node"
    }
  ],
  "alias":  ["inkglass.now.sh"],
 "routes": [
    {
      "src": "/back-end/journalapi/(.*)",
      "dest": "back-end/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/app/$1"
    }
  ],
  "github": {
    "enabled": true
  }
}