{
  "name": "inkglass",
  "version": 2,
  "alias": ["inkglass.now.sh"],
  "builds": [
    {
      "src": "back-end/journalapi/index.js",
      "use": "@now/node"
    },
    { "src": "front-end/package.json", "use": "@now/node" }
  ],
  "routes": [
    {
      "src": "/back-end/journalapi/(.*)",
      "dest": "back-end/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/app/$1"
    }
  ],
  "github": {
    "enabled": true
  }
}

